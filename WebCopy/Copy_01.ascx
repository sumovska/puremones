<script runat="server">
	protected override void OnLoad(EventArgs e)
	{
		System.Collections.Generic.List<HtmlTableRow> list = new System.Collections.Generic.List<HtmlTableRow>(TESTEMONIAL_TABLE.Rows.Count);
		foreach (HtmlTableRow row in TESTEMONIAL_TABLE.Rows) list.Add(row);

		TESTEMONIAL_TABLE.Rows.Clear();

		//testemonials are randomized based on url and time
		int seed = 0;
		foreach (char c in Request.RawUrl)
		{
			seed += (int)c;
		}

		seed += DateTime.Now.DayOfYear*10000 + (int)DateTime.Now.TimeOfDay.TotalMinutes;

		Random r=new Random(seed);

		list.RemoveAt(r.Next(list.Count));

		//50 percent chance that will show only 5 testimonials
		if (r.Next(100) < 50) list.RemoveAt(r.Next(list.Count));
		//20 percent chance that will show only 4 testimonials
		if (r.Next(100) < 20) list.RemoveAt(r.Next(list.Count));

		while (list.Count > 0)
		{
			int ix = r.Next(list.Count);
			TESTEMONIAL_TABLE.Rows.Add(list[ix]);
			list.RemoveAt(ix);
		}

		base.OnLoad(e);
	}
</script>